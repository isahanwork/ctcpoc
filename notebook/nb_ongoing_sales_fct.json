					"%%pyspark\r\n",
					"\r\n",
					"def read_raw_table():\r\n",
					"\r\n",
					"    print(esp_date)\r\n",
					"\r\n",
					"    spark.conf.set(\"esp_date\", esp_date)  \r\n",
					"\r\n",
					"    sys = mssparkutils.notebook\r\n",
					"\r\n",
					"\r\n",
					"    try:\r\n",
					"        raw_sales_df = spark.read.parquet(f\"{base_path}/raw/sales/batch_dt={esp_date}/*.parquet\")\r\n",
					"    except:\r\n",
					"        sys.exit(f\"cannot load batch_dt={esp_date} file\")  \r\n",
					"\r\n",
					"    raw_sales_df.createOrReplaceTempView('ref_raw_sales')\r\n",
					"    raw_sales_df.show()"
				],
